<?xml version="1.0" encoding="UTF-8"?><services-config>    <services>        <default-channels>            <channel ref="my-amf"/>        </default-channels>    </services>    <security>        <security-constraint id="trusted">            <auth-method>Custom</auth-method>            <roles>                <role>mainapp</role>            </roles>        </security-constraint>    </security>    <channels>        <channel-definition id="my-amf" class="mx.messaging.channels.AMFChannel">            <endpoint                    url="http://{server.name}:{server.port}/{context.root}/messagebroker/amf"                    class="flex.messaging.endpoints.AMFEndpoint"/>        </channel-definition>    </channels>    <system>        <redeploy>            <enabled>false</enabled>        </redeploy>    </system></services-config>